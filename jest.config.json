{
  "testEnvironment": "jest-environment-jsdom",
  "moduleNameMapper": {
    "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/src/test/fileMock.js",
    "\\.(css|less)$": "identity-obj-proxy"
  },
  "setupFilesAfterEnv": ["<rootDir>/src/test/setup-jest.js"],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
